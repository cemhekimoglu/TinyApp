<html lang="en">
<head>
  <title>TinyApp</title>
</head>
<body>
<header>
  <% include partials/_header.ejs %>
</header>
  <% if (user !== undefined) { %>
  <ul>
    <% for(var name in urls) { %>

      <li>
        <%= name %> : <a href="<%=urls[name]%>"><%= urls[name]["url"]%> </a>
        <a href="/urls/<%=name%>">Edit </a>
        <form method="POST" action="/urls/<%= name %>/delete"> <input type="SUBMIT" value="DELETE"></form>

      </li>
     <% } %>

  </ul>
  <a href="http://localhost:8080/urls/new">Let's Create Shorter links!</a>
  <% } %>



</body>
</html>
